<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.footballtime.api.dao.SeasonTeamRepository">
  <sql id="Base_Column_List">
    TeamNo,Name,FullName,CompetitionId,IsClub,PicNo
  </sql>
  <select id="getTeamListBySeason" parameterType="map" resultMap="BaseResultMap">
    select <include refid="Base_Column_List" /> from SeasonTeam where CompetitionId=#{competitionId} and Season = #{Season}
  </select>
</mapper>